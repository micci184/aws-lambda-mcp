# AWS Region
aws_region = "ap-northeast-1"

# Lambda function name
function_name = "aws-api-mcp-server"

# ECR repository name
ecr_repository_name = "aws-api-mcp-server"

# Docker image tag
image_tag = "latest"

# Lambda timeout (seconds)
timeout = 300

# Lambda memory (MB)
memory_size = 512

# Tags
tags = {
  Project     = "aws-lambda-mcp"
  Environment = "dev"
}

